algoritmo "Number_before_after"
// =============================================================
//
//
// AUTOR: Vinicius Bayão
// Curso: Algoritmos(Curso em Vídeo)
// =============================================================
var
   n1, a, s: inteiro
inicio

      Escreva("Digite um número: ")
      Leia(n1)
      a <- n1 - 1
      s <- n1 + 1
      Escreval("O antecessor de", n1," é", a)
      Escreval("O sucessor de", n1," é", s)
      

fimalgoritmo
